.ui.inverted.menu{
  background: @brandColor;
  border-radius: 0px;
}

.right.menu {
    padding-right: 0em !important;
}

.ui.vertical.menu .active.item {
    background: #2185d0;
    pointer-events: none;
}

.ui.menu .blue.active.item {
    color: white !important;
}

.ui.vertical.menu .active.item:hover {
    background-color: #2185d0;
}

